{
  "name": "react-auto-router",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "description": "A plugin to create router elegantly",
  "author": {
    "name": "Ohh",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/mufeng889"
  },
  "contributors": [
    {
      "name": "Soybean",
      "email": "soybeanjs@outlook.com",
      "url": "https://github.com/soybeanjs"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/elegant-router",
  "scripts": {
    "dev": "vite",
    "build": "pnpm typecheck && pnpm unbuild",
    "preview": "vite preview",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "lint": "eslint . --fix",
    "publish-pkg": "pnpm -r publish --access public",
    "release": "pnpm soy release",
    "stub": "pnpm -r --parallel run stub",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "consola": "3.2.3",
    "gradient-string": "2.0.2",
    "prettier": "3.3.3",
    "recast": "0.23.9",
    "micromatch": "4.0.7",
    "magicast": "0.3.4",
    "unplugin": "1.12.0"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.6",
    "@types/micromatch": "4.0.9",
    "@types/node": "20.14.12",
    "react-router-dom": "6.25.1",
    "@soybeanjs/cli": "1.0.19",
    "unbuild": "2.0.0",
    "typescript": "5.5.4",
    "vite": "5.3.5"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
