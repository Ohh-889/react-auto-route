{
  "name": "@soybean-react/vite-plugin-react-router",
  "type": "module",
  "version": "0.7.1",
  "packageManager": "pnpm@9.1.1",
  "description": "A plugin to create router elegantly",
  "author": {
    "name": "Ohh",
    "email": "1509326266@qq.com",
    "url": "https://github.com/mufeng889"
  },
  "license": "MIT",
  "homepage": "https://github.com/mufeng889/react-auto-route",
  "repository": {
    "url": "https://github.com/mufeng889/react-auto-route.git"
  },
  "bugs": {
    "url": "https://github.com/mufeng889/react-auto-route/issues"
  },
  "keywords": [
    "react-router",
    "react",
    "router",
    "auto-route",
    "contract-router",
    "react-router-dom"
  ],
  "contributors": [
    {
      "name": "Soybean",
      "email": "soybeanjs@outlook.com",
      "url": "https://github.com/soybeanjs"
    }
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup  --minify && tsx scripts/copyRefreshUtils.ts",
    "dev": "unbuild --stub",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "publish-pkg": "pnpm -r publish --access public --no-git-checks",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@babel/core": "7.26.9",
    "@babel/plugin-transform-react-jsx-self": "7.25.9",
    "@babel/plugin-transform-react-jsx-source": "7.25.9",
    "@types/babel__core": "7.20.5",
    "chokidar": "3.6.0",
    "consola": "3.2.3",
    "dedent": "1.5.3",
    "es-module-lexer": "1.6.0",
    "fast-glob": "3.3.2",
    "gradient-string": "2.0.2",
    "kolorist": "1.8.0",
    "magicast": "0.3.5",
    "micromatch": "4.0.7",
    "prettier": "3.4.2",
    "react-refresh": "0.16.0",
    "recast": "0.23.11",
    "unplugin": "1.12.0"
  },
  "devDependencies": {
    "@types/gradient-string": "1.1.6",
    "@types/micromatch": "4.0.9",
    "@types/node": "22.14.1",
    "react-router-dom": "7.1.1",
    "tsup": "8.4.0",
    "tsx": "^4.20.5",
    "unbuild": "3.5.0",
    "vite": "6.2.6"
  }
}
